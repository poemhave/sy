using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class KimSiYoung : MonoBehaviour
{
    // Start is called before the first frame update
    void Start()
    {
        string userinput = "40";
        int number = int.Parse(userinput);

        if (number >= 50)
        {
            Debug.Log("50이상의 수");
        }
        else if (number < 50)
        {
            Debug.Log("50미만의 수");
        }
        
        }

    // Update is called once per frame
    void Update()
    {
        
    }
}
